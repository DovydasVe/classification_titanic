    "pipeline_lgb = Pipeline([\n",
    "    ('preprocessor', preprocessor),\n",
    "    ('classifier', LGBMClassifier(random_state=42, verbose=-1))\n",
    "])\n",
    "\n",
    "param_grid_lgb = {\n",
    "    'classifier__num_leaves': randint(20, 50),\n",
    "    'classifier__max_depth': randint(4, 12),\n",
    "    'classifier__learning_rate': [0.01, 0.05, 0.1],\n",
    "    'classifier__n_estimators': randint(100, 300),\n",
    "}\n",
    "\n",
    "model_lgb = RandomizedSearchCV(pipeline_lgb, param_distributions=param_grid_lgb, \n",
    "                                   n_iter=50, cv=cv, scoring='accuracy', verbose=0, n_jobs=-1, random_state=42)\n",
    "model_lgb.fit(X_train, y_train)\n",
    "y_pred_lgb = model_lgb.predict(X_test)\n",
